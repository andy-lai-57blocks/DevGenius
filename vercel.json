{
  "name": "devgenius",
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "installCommand": "corepack enable && yarn install",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
