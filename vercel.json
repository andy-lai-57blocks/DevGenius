{
  "name": "one-toys",
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "installCommand": "corepack enable && yarn install",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/ads.txt",
      "dest": "/ads.txt"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        
        "Content-Security-Policy": "script-src * 'unsafe-inline' 'unsafe-eval'; img-src *; frame-src *; connect-src *; style-src * 'unsafe-inline'; font-src *; media-src *; object-src *; default-src *;"
      }
    }
  ]
}
